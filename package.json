{
  "name": "@anvilapp/rn-localization",
  "version": "0.3.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "yura.pustylnikov@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/pustylnikov/rn-localization",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint-fix-build": "eslint ./dist --ext .js,.jsx,.ts,.tsx --fix",
    "build": "yarn run test && rm -rf ./dist && tsc --project tsconfig.build.json && yarn run lint-fix-build",
    "release-dry": "yarn run build && npm publish --access public --dry-run",
    "release": "yarn run build && npm publish --access public"
  },
  "keywords": [
    "react-native",
    "react native",
    "localization"
  ],
  "peerDependencies": {
    "react": ">=16",
    "react-native": ">=0.60"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4",
    "react": "17.0.2",
    "react-native": "0.68.1"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "files": [
    "dist/",
    "README.md"
  ]
}
